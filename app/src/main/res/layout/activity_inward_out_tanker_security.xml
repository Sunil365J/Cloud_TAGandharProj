<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".InwardOut_Tanker_Security"
    >


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="450dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        android:orientation="vertical"
        android:gravity="center">

        <androidx.cardview.widget.CardView
            android:layout_width="405dp"
            android:layout_height="46dp"
            android:layout_weight="2">

            <com.google.android.material.textfield.TextInputLayout
                android:layout_width="185dp"
                android:layout_height="wrap_content"
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="30dp"
                android:layout_marginTop="10dp"

                >
                <EditText
                    android:id="@+id/etintime"
                    android:layout_width="185dp"
                    android:layout_height="wrap_content"
                    android:hint="In Time"
                    android:inputType="text"
                    android:textSize="17dp"
                    android:textStyle="bold"
                    android:focusable="false"
                    android:clickable="false"
                    />
            </com.google.android.material.textfield.TextInputLayout>


        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:layout_width="405dp"
            android:layout_height="46dp"
            android:layout_weight="2">
            <com.google.android.material.textfield.TextInputLayout
                android:layout_width="185dp"
                android:layout_height="wrap_content"
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="30dp"
                android:layout_marginTop="10dp"
                android:id="@+id/regno"
                android:gravity="center"
                >
                <EditText
                    android:id="@+id/ettransportlr"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="Transport LR Copy"
                    android:inputType="text"
                    android:textSize="17dp"
                    android:textStyle="bold"
                    android:textColor="#322C4A"
                    android:focusable="false"
                    android:clickable="false"/>
            </com.google.android.material.textfield.TextInputLayout>

            <com.google.android.material.textfield.TextInputLayout
                android:layout_width="185dp"
                android:layout_height="wrap_content"
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                android:layout_marginLeft="200dp"
                android:layout_marginRight="30dp"
                android:layout_marginTop="10dp"
                android:gravity="center"
                >
                <EditText
                    android:id="@+id/ettrwvehicalno"
                    android:layout_width="185dp"
                    android:layout_height="wrap_content"
                    android:hint="View Image"
                    android:inputType="text"
                    android:textSize="17dp"
                    android:textStyle="bold"
                    />
            </com.google.android.material.textfield.TextInputLayout>
        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/wtwo"
            android:layout_width="405dp"
            android:layout_height="55dp"

            android:layout_weight="2"
            >
            <com.google.android.material.textfield.TextInputLayout
                android:layout_width="185dp"
                android:layout_height="wrap_content"
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="30dp"
                android:layout_marginTop="10dp"

                >
                <EditText
                    android:id="@+id/etdeliverychallan"
                    android:layout_width="185dp"
                    android:layout_height="wrap_content"
                    android:hint="Delivery Challan"
                    android:inputType="text"
                    android:textSize="17dp"
                    android:textStyle="bold"
                    />
            </com.google.android.material.textfield.TextInputLayout>

            <com.google.android.material.textfield.TextInputLayout
                android:layout_width="185dp"
                android:layout_height="wrap_content"
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                android:layout_marginLeft="200dp"
                android:layout_marginRight="30dp"
                android:layout_marginTop="10dp"

                >
                <EditText
                    android:id="@+id/ettrwmaterial"
                    android:layout_width="185dp"
                    android:layout_height="wrap_content"
                    android:hint="View Image"
                    android:inputType="text"
                    android:textSize="17dp"
                    android:textStyle="bold"
                    />
            </com.google.android.material.textfield.TextInputLayout>

        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/wthree"
            android:layout_width="405dp"
            android:layout_height="55dp"

            android:layout_weight="2">

            <com.google.android.material.textfield.TextInputLayout
                android:layout_width="185dp"
                android:layout_height="wrap_content"
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="30dp"
                android:layout_marginTop="10dp"

                >
                <EditText
                    android:id="@+id/ettaxinvoice"
                    android:layout_width="185dp"
                    android:layout_height="wrap_content"
                    android:hint="Tax Invoice"
                    android:inputType="text"
                    android:textSize="17dp"
                    android:textStyle="bold"
                    />
            </com.google.android.material.textfield.TextInputLayout>

            <com.google.android.material.textfield.TextInputLayout
                android:layout_width="185dp"
                android:layout_height="wrap_content"
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                android:layout_marginLeft="200dp"
                android:layout_marginRight="30dp"
                android:layout_marginTop="10dp"

                >
                <EditText
                    android:id="@+id/ettrdriver"
                    android:layout_width="185dp"
                    android:layout_height="wrap_content"
                    android:hint="View Image"
                    android:inputType="number"
                    android:textSize="17dp"
                    android:textStyle="bold"/>
            </com.google.android.material.textfield.TextInputLayout>
        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/wfour"
            android:layout_width="405dp"
            android:layout_height="55dp"

            android:layout_weight="2"
            >
            <com.google.android.material.textfield.TextInputLayout
                android:layout_width="185dp"
                android:layout_height="wrap_content"
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="30dp"
                android:layout_marginTop="10dp"

                >
                <EditText
                    android:id="@+id/etewaybill"
                    android:layout_width="185dp"
                    android:layout_height="wrap_content"
                    android:hint="E-Way Bill"
                    android:inputType="text"
                    android:textSize="17dp"
                    android:textStyle="bold"/>
            </com.google.android.material.textfield.TextInputLayout>

            <com.google.android.material.textfield.TextInputLayout
                android:layout_width="185dp"
                android:layout_height="wrap_content"
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                android:layout_marginLeft="200dp"
                android:layout_marginRight="30dp"
                android:layout_marginTop="10dp"

                >
                <EditText
                    android:id="@+id/ettrdate"
                    android:layout_width="185dp"
                    android:layout_height="wrap_content"
                    android:hint="View Image"
                    android:inputType="text"
                    android:textSize="17dp"
                    android:textStyle="bold"
                    android:focusable="false"/>
            </com.google.android.material.textfield.TextInputLayout>
        </androidx.cardview.widget.CardView>








    </LinearLayout>
</RelativeLayout>